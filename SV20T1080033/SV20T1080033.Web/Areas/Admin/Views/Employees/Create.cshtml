@model SV20T1080033.DomainModels.Employee;
<div class="box box-primary">

    <div class="box-body">

        <form action="~/Admin/Employee/Save">

            
            <div class="form-group">
                <label>Họ tên:</label>
              
                <input type="text" name="@nameof(Model.FullName)" value="@Model.FullName" class="form-control" autofocus>
            </div>
            <div class="form-group">
                <label>Ngày sinh:</label>
                <input type="text" name="@nameof(Model.Birthday)" value="@Model.Birthday" class="form-control">
            </div>
            <div class="form-group">
                <label>Địa chỉ:</label>
                <input type="text" name="@nameof(Model.Address)" value="@Model.Address" class="form-control">
            </div>
            <div class="form-group">
                <label>Điện thoại:</label>
                <input type="text" name="@nameof(Model.Phone)" value="@Model.Phone" class="form-control">
            </div>
            
            <div class="form-group">
                <label>Email:</label>
                <input type="text" name="@nameof(Model.Email)" value="@Model.Email" class="form-control">
            </div>
            <div class="form-group">
                
                <input type="checkbox" name="@nameof(Model.IsWorking)" value="@Model.IsWorking" checked >
                <label for="vehicle1"> Nhân viên đang làm việc</label><br>
            </div>
           
            <div class="form-group">
                <label>Ảnh cả nhân: </label>
                <input type="file" id="myFile" name="filename">
                <img style="margin-top:20px" src="data:image/png;base64,"
                     alt="Girl in a jacket" width="160" height="200">
            </div>

            <button type="submit" class="btn btn-primary">
                <i class="fa fa-floppy-o"></i>
                Lưu dữ liệu
            </button>

            <button type="submit" class="btn btn-default">
                <a href="~/Admin/Employees">
                    Quay lại
                </a>

            </button>
        </form>

    </div>
</div>

