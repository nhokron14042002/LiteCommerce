@{
    ViewBag.Title = " Danh sách nhân viên";
}


<div class="box box-primary">
    <div class="box-body">
        <form action="#" method="get">
            <div class="input-group">
                <input type="text" name="searchValue" class="form-control" placeholder="Nhập tên nhân viên cần tim.." spellcheck="false" data-ms-editor="true">
                <span class="input-group-btn">
                    <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                        <i class="fa fa-search"></i>
                    </button>
                    <a href="~/Admin/Employees/Create" class="btn btn-xs btn-primary">
                        <i class="fa fa-plus"></i> Bổ sung
                    </a>
                </span>
            </div>
        </form>
        @* danh sach nhà cung cấp *@
        <p style="margin:10px 0 10px 0">
            Có <strong>1,234 </strong>nhân viên, Hiện đang ở trang <strong>1</strong> trên tổng số <strong>200</strong>
        </p>



        @for (int i = 1; i <= 4; i++)
        {
            <div class="row">
                @for (int j = 1; j <= 3; j++)
                {
                    <div class="col-xs-12 col-sm-4 col-md-4">
                        <div class="well well-sm">
                            <div class="row">
                                <div class="col-sm-6 col-md-4">
                                    <img style="margin: 30px 50px" src="data:image/png;base64," alt="" class="img-rounded img-responsive" />


                                </div>
                                <div class="col-sm-6 col-md-8">
                                    <h3>
                                        Trần Văn Hiếu
                                        <small style="margin-left:80px" >
                                            <a href="~/Admin/Employees/Edit/123" class="btn btn-xs btn-info">
                                                <i class="fa fa-edit"></i>
                                            </a>

                                            <a href="~/Admin/Employees/Repass/123" class="btn btn-xs btn-key">
                                                <i class="fa fa-key"></i>
                                            </a>

                                            <a href="~/Admin/Employees/delete/123" class="btn btn-xs btn-danger">
                                                <i class="fa fa-remove"></i>
                                            </a>
                                        </small>

                                    </h3>
                                    <small>
                                        <cite title="San Francisco, USA">
                                            <i class="glyphicon glyphicon-map-marker">
                                                77 Nguyễn Huệ ,Thành phố Huế
                                            </i>
                                        </cite>
                                    </small>
                                    <p>
                                        <i class="glyphicon glyphicon-gift"> 02/09/2002</i>
                                        <br />
                                        <i class="glyphicon glyphicon-envelope"> tranvanhieu@gmail.com</i>
                                        <br />
                                        <i class="glyphicon glyphicon-phone"> 09938376346</i>
                                        <br />

                                    </p>
                                    <!-- Split button -->
                                    <div class="btn-group">
                                       


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                }

            </div>
        }



    </div>
</div>

